<?xml version="1.0" encoding="UTF-8"?>
<CHECKLIST_SET>
    <CHECKLIST alias="test_checklist_alias_1" center_name="EBI" accession="checklist">
        <DESCRIPTOR>
            <CHECKLIST_AUTHORITY>ENA</CHECKLIST_AUTHORITY>
            <CHECKLIST_NAME>test_checklist_name_1</CHECKLIST_NAME>
            <CHECKLIST_DESCRIPTION>A test checklist</CHECKLIST_DESCRIPTION>
            <CHECKLIST_VERSION>1</CHECKLIST_VERSION>
            <CHECKLIST_TYPE>sample</CHECKLIST_TYPE>
            <CHECKLIST_GROUP>
                <GROUP>non-sample term</GROUP>
                <DESCRIPTION/>
                <CHECKLIST_ATTRIBUTE>
                    <TAG>test_mandatory</TAG>
                    <DESCRIPTION>This is to test mandatory tag. This attribute is mandatory.
                    </DESCRIPTION>
                    <MANDATORY>mandatory</MANDATORY>
                    <VALUE>
                        <TEXT_VALUE/>
                    </VALUE>
                </CHECKLIST_ATTRIBUTE>
                <CHECKLIST_ATTRIBUTE>
                    <TAG>test_recommended</TAG>
                    <DESCRIPTION>This is to test recommended tags. This attribute is recommended.
                    </DESCRIPTION>
                    <MANDATORY>recommended</MANDATORY>
                    <VALUE>
                        <TEXT_VALUE/>
                    </VALUE>
                </CHECKLIST_ATTRIBUTE>
                <CHECKLIST_ATTRIBUTE>
                    <TAG>test_multiplicity</TAG>
                    <DESCRIPTION>This is to test multiplicity tag. This attribute should appear 0 or
                        1 time.
                    </DESCRIPTION>
                    <MANDATORY>optional</MANDATORY>
                    <MULTIPLICITY>single</MULTIPLICITY>
                    <VALUE>
                        <TEXT_VALUE/>
                    </VALUE>
                </CHECKLIST_ATTRIBUTE>
                <CHECKLIST_ATTRIBUTE>
                    <TAG>test_null_value</TAG>
                    <DESCRIPTION>This is to test the null value when regex required. This attribute
                        value should match the regex, so cannot be null.
                    </DESCRIPTION>
                    <MANDATORY>mandatory</MANDATORY>
                    <VALUE>
                        <REGEXP_VALUE>[+-]?[0-9]+.?[0-9]*</REGEXP_VALUE>
                    </VALUE>
                </CHECKLIST_ATTRIBUTE>
                <CHECKLIST_ATTRIBUTE>
                    <TAG>test_regex_value</TAG>
                    <DESCRIPTION>This is to test the regex value when required. This attribute value
                        should match the regex.
                    </DESCRIPTION>
                    <MANDATORY>optional</MANDATORY>
                    <VALUE>
                        <REGEXP_VALUE>[+-]?[0-9]+.?[0-9]*</REGEXP_VALUE>
                    </VALUE>
                </CHECKLIST_ATTRIBUTE>
                <CHECKLIST_ATTRIBUTE>
                    <TAG>test_invalid_value</TAG>
                    <DESCRIPTION>This is to test the invalid value. This attribute value should be
                        one of the choices available.
                    </DESCRIPTION>
                    <MANDATORY>optional</MANDATORY>
                    <VALUE>
                        <TEXT_CHOICE>
                            <VALUE>possible_value_1</VALUE>
                            <VALUE>possible_value_2</VALUE>
                        </TEXT_CHOICE>
                    </VALUE>
                </CHECKLIST_ATTRIBUTE>
                <CHECKLIST_ATTRIBUTE>
                    <TAG>test_invalid_unit</TAG>
                    <DESCRIPTION>This is to test the invalid unit. This attribute unit should be the
                        unit specified here.
                    </DESCRIPTION>
                    <MANDATORY>optional</MANDATORY>
                    <VALUE>
                        <TEXT_VALUE/>
                    </VALUE>
                    <UNITS>
                        <UNIT>valid_unit</UNIT>
                    </UNITS>
                </CHECKLIST_ATTRIBUTE>
            </CHECKLIST_GROUP>
        </DESCRIPTOR>
    </CHECKLIST>
</CHECKLIST_SET>